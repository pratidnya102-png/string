#include <stdio.h>
   int main() {
    char a[20], b[20], c[20];
    int i, j;

    printf("Enter first code: ");
    scanf("%s", a);
    printf("Enter second code: ");
    scanf("%s", b);

    // Length of a
    for (i = 0; a[i] != '\0'; i++);
    printf("\nLength of first code: %d", i);

    // Copy a to c
    for (j = 0; a[j] != '\0'; j++) c[j] = a[j];
    c[j] = '\0';
    printf("\nCopy of first code: %s", c);

    // Compare a and b
    for (i = 0; a[i] == b[i] && a[i] != '\0'; i++);
    if (a[i] == b[i]) printf("\nBoth codes are SAME");
    else printf("\nBoth codes are NOT same");

    // Concatenate b to a
    for (i = 0; a[i] != '\0'; i++);
    for (j = 0; b[j] != '\0'; j++, i++) a[i] = b[j];
    a[i] = '\0';
    printf("\nConcatenated code: %s", a);

    return 0;
}


